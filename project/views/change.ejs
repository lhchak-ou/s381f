<html>
<body>

<% res_value.forEach(function(res_value){ %>
  <% res_address.forEach(function(res_address){ %>
<h1>Modify Restaurant <%= res_value.name %></h1>
<form action="/change_h" method="post" enctype="multipart/form-data" class="w3-container" style="width:100%">
  <p>
  <input type="hidden" name="_id" value=<%= id %>>
  <input type="hidden" name="userid" value=<%= username %>>

  <input name="name" class="w3-input" type="text" value= <%= res_value.name %>>
  <label class="w3-label w3-validate">Name</label></p>
  <p>
  <input name="borough" class="w3-input" type="text" value= <%= res_value.borough %>>
  <label class="w3-label w3-validate">Borough</label></p>
  <p>
  <input name="cuisine" class="w3-input" type="text" value=<%= res_value.cuisine %>>
  <label class="w3-label w3-validate">Cuisine</label></p>
  <p>
  <input name="street" class="w3-input" type="text" value=<%= res_address.street %>>
  <label class="w3-label w3-validate">Street</label></p>
  <p>
  <input name="building" class="w3-input" type="text" value=<%= res_address.building %>>
  <label class="w3-label w3-validate">Building</label></p>
  <p>
  <input name="zipcode" class="w3-input" type="text" value=<%= res_address.zipcode %>>
  <label class="w3-label w3-validate">Zipcode</label></p>
  <p>
  </p><p>
  <input name="lon" class="w3-input" type="text" value= <%= res_address.coord[0] %>>
  <label class="w3-label w3-validate">GPS Coordinates (lon.)</label></p>
  <p>
  <input name="lat" class="w3-input" type="text" value= <%= res_address.coord[1] %>>
  <label class="w3-label w3-validate">GPS Coordinates (lat.)</label></p>
  <p>
  <input name="sampleFile" class="w3-input" type="file" accept='image/*'>
  <label class="w3-label w3-validate">Photo</label></p>
   <% }); %>
   <% }); %>
  <p>
  <button class="w3-btn w3-section w3-teal w3-ripple"> Change </button></p>
</form>







</body>
</html>
